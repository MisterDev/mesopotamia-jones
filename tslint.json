{
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "rules": {
        "member-access": false,
        "object-literal-sort-keys": false,
        "interface-name": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "jsx-boolean-value": false,
        "no-submodule-imports": false
    }
}